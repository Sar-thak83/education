{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "@VITE_API_BASE_URL"
  }
}
